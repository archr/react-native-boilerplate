{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "jsx-quotes": "prefer-single",
    "react/prop-types": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-indent-props": 2,
    "react/jsx-closing-bracket-location": [2, "tag-aligned"]
  }
}
